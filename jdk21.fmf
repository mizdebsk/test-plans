environment:
  OPENJDK_VERSION: 21
  OPENJDK_HEADLESS: "false"
prepare:
  - name: mbici-install
    how: install
    package:
     - ant-openjdk21
     - maven-openjdk21
tag:
  - matrix
discover:
  - name: tests/ant
    how: fmf
    url: https://src.fedoraproject.org/tests/ant.git
    filter: tag:jdk21
  - name: tests/maven
    how: fmf
    url: https://src.fedoraproject.org/tests/maven.git
    filter: tag:jdk21
  - name: tests/javapackages-tools
    how: fmf
    url: https://src.fedoraproject.org/tests/javapackages-tools.git
    filter: tag:jdk21
execute:
  how: tmt
