environment:
  OPENJDK_VERSION: 1.8.0
  OPENJDK_HEADLESS: "true"
prepare:
  - name: mbici-install
    how: install
    package:
     - maven-openjdk8
  - name: mbici-erase
    how: shell
    script: dnf -y remove java-*-openjdk
discover:
