    how: fmf
    url: https://src.fedoraproject.org/rpms/ant.git
    filter: tag:jre8
  - name: maven
    how: fmf
    url: https://src.fedoraproject.org/rpms/maven.git
    filter: tag:jre8
  - name: byaccj
    how: fmf
    url: https://src.fedoraproject.org/rpms/byaccj.git
    filter: tag:jre8
  - name: javacc
    how: fmf
    url: https://src.fedoraproject.org/rpms/javacc.git
    filter: tag:jre8
  - name: jctools
    how: fmf
    url: https://src.fedoraproject.org/rpms/jctools.git
    filter: tag:jre8
  - name: jflex
    how: fmf
    url: https://src.fedoraproject.org/rpms/jflex.git
    filter: tag:jre8
  - name: maven-surefire
    how: fmf
    url: https://src.fedoraproject.org/rpms/maven-surefire.git
    filter: tag:jre8
  - name: modello
    how: fmf
    url: https://src.fedoraproject.org/rpms/modello.git
